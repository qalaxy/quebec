(function($) {

  "use strict";

  // caching selectors
  var mainWindow         = $(window),
    mainNav              = $('#navigation'),
    mainBody             = $('body'),
    mainStatus           = $('#status'),
    mainPreloader        = $('#preloader'),
    scrollUp             = $('.scrollup'),
    onSearch             = $('#search-icon'),
    Search               = $('#search'),
    emailSubscribe       = $('#edit-mail-0-value'),
    removeNotification   = $('#notification-close'),
    notification         = $('#block-views-block-front-notification-block-1');





  mainWindow.on('load', function() {

    // Preloader
    mainStatus.fadeOut();
    mainPreloader.delay(350).fadeOut('slow');
    mainBody.delay(350).css({
      'overflow': 'visible'
    });

    // Scroll to Top
    mainWindow.on("scroll", function() {
      if ($(this).scrollTop() > 98){
        mainNav.addClass("sticky");
        mainBody.addClass("sticky");
        scrollUp.show();
      }
      else{
        mainNav.removeClass("sticky");
        mainBody.removeClass("sticky");
        scrollUp.hide();
      }
    });

    // Click event to scroll to top
    scrollUp.on("click", function() {
      $('html, body').animate({
        scrollTop: 0
      }, 800);
      return false;
    });

    // Email Subscription placeholder
    emailSubscribe.attr('placeholder', 'Your Email');

    // on Search Click
    onSearch.on("click", function() {
      Search.toggleClass("toggle_search_show");
